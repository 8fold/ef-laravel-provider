{
	"name": "8fold/ef-laravel-provider",
  "description": "A generic Laravel Service Provider and resources for creating Laravel sites",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": "7.*",
		"8fold/laravel-markup": "~0.0.27",
		"8fold/php-events": "~0.0.0",
		"8fold/php-schema": "~0.0.4",
		"jaybizzle/crawler-detect": "^1.2"
	},
	"require-dev": {
		"orchestra/testbench": "~5.3",
		"orchestra/testbench-browser-kit": "~5.1",
		"phpunit/phpunit": "~9.1"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Eightfold\\Site\\": "./src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
      "Eightfold\\Site\\Tests\\": "./tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Eightfold\\Site\\Provider"
			],
			"dont-discover": []
		}
	},
  "minimum-stability": "beta",
	"prefer-stable": true
}
